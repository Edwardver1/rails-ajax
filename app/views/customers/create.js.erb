
$(".form-new").on("submit", function(e) {
  $.ajax({
    url: $(this).attr('action'),
    data: $(this).serialize(),
    type: "POST",
    dataType: "json",
    success: function(response) {
      console.log(response)
    },
    error: function(xhr, textStatus, errorThrown) {

    }
  });
  e.preventDefault(); //THIS IS VERY IMPORTANT
});
